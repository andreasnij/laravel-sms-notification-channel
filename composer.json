{
    "name": "andreasnij/laravel-sms-notification-channel",
    "type": "library",
    "description": "An SMS notification channel for the PHP framework Laravel.",
    "keywords": [
        "sms",
        "laravel",
        "notification",
        "46elks",
        "cellsynt",
        "twilio",
        "vonage"
    ],
    "homepage": "http://github.com/andreasnij/laravel-sms-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Nilsson"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/contracts": "^7.0 ||^8.0 ",
        "illuminate/notifications": "^7.0 || ^8.0",
        "jandreasn/an-sms": "dev-next"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.23",
        "twilio/sdk": "^6.32",
        "vonage/client-core": "^2.10",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelSmsNotificationChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSmsNotificationChannel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs",
        "phpunit": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@phpcs",
            "@phpstan",
            "@phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSmsNotificationChannel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    }
}
